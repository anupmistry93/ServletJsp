<?xml version="1.0" encoding="UTF-8"?>
<web-app>


<!-- .............url redirect..........   -->
<servlet>
  <servlet-name>play1</servlet-name>
  <servlet-class>com.urlRedirect1.Servlet1</servlet-class>
</servlet>


 <servlet-mapping>
  <servlet-name>play1</servlet-name>
  <url-pattern>/addition</url-pattern>
</servlet-mapping>


<servlet>
  <servlet-name>play2</servlet-name>
  <servlet-class>com.urlRedirect1.Servlet2</servlet-class>
</servlet>


 <servlet-mapping>
  <servlet-name>play2</servlet-name>
  <url-pattern>/square</url-pattern>
</servlet-mapping>

<!-- .............session...........   -->
<servlet>
  <servlet-name>play3</servlet-name>
  <servlet-class>com.session.Servlet1</servlet-class>
</servlet>


 <servlet-mapping>
  <servlet-name>play3</servlet-name>
  <url-pattern>/addition1</url-pattern>
</servlet-mapping>


<servlet>
  <servlet-name>play4</servlet-name>
  <servlet-class>com.session.Servlet2</servlet-class>
</servlet>


 <servlet-mapping>
  <servlet-name>play4</servlet-name>
  <url-pattern>/square1</url-pattern>
</servlet-mapping>
<!-- .............cookies...........   -->


</web-app>